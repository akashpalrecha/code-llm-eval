accelerate launch  main.py \
  --model deepseek-ai/deepseek-coder-1.3b-base \
  --max_length_generation 512 \
  --tasks humaneval \
  --temperature 0.2 \
  --n_samples 10 \
  --batch_size 10 \
  --allow_code_execution \
  --limit 5


accelerate launch  main.py \
  --model deepseek-ai/deepseek-coder-1.3b-base \
  --max_length_generation 512 \
  --tasks mbpp \
  --temperature 0.1 \
  --n_samples 5 \
  --batch_size 5 \
  --allow_code_execution \
  --limit 5


accelerate launch  main.py \
  --model deepseek-ai/deepseek-coder-1.3b-base  \
  --tasks multiple-go  \
  --max_length_generation 650 \
  --temperature 0.8   \
  --do_sample True  \
  --n_samples 5  \
  --batch_size 5  \
  --trust_remote_code \
  --generation_only \
  --save_generations \
  --save_generations_path generations_all.json \
  --limit 5

